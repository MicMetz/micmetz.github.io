				"babel-plugin-styled-components": "1.10.2",
				"chrome-aws-lambda": "^9.1.0",
				"crypto": "^1.0.1",
				"cssnano": "^6.0.1",
				"d3": "^7.8.5",
				"date-fns": "^2.30.0",
				"dotenv": "^16.1.4",
				"feed": "^4.2.2",
				"framer-motion": "^4.1.17",
				"gh-pages": "^5.0.0",
				"gray-matter": "^4.0.3",
				"gsap": "^3.8.0",
				"imagemagick": "^0.1.3",
				"instantsearch.css": "^7.4.5",
				"lodash": "^4.17.21",
				"next": "^12.3.4",
				"next-mdx-remote": "^4.4.1",
				"next-pwa": "^5.6.0",
				"next-seo": "^6.0.0",
				"playwright": "^1.34.3",
				"playwright-aws-lambda": "^0.7.0",
				"playwright-core": "^1.17.1",
				"prism-react-renderer": "^1.2.1",
				"react": "^17.0.2",
				"react-collapsed": "^3.3.2",
				"react-dom": "^17.0.2",
				"react-gsap": "^3.2.1",
				"react-icons": "^4.8.0",
